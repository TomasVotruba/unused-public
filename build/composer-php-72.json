{
    "name": "tomasvotruba/unused-public",
    "type": "phpstan-extension",
    "description": "Detect unused public properties, constants and methods in your code",
    "license": "MIT",
    "require": {
        "php": "^7.2 || 8.0.*",
        "../vendor/phpstan/phpstan": "^1.9.3",
        "../vendor/nette/utils": "^3.2",
        "../vendor/webmozart/assert": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "TomasVotruba\\UnusedPublic\\": "src"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "config/extension.neon"
            ]
        }
    }
}

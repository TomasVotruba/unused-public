includes:
    - ../../../../config/included_services.neon

rules:
    - Symplify\PHPStanRules\Rules\DeadCode\UnusedPublicStaticPropertyRule

services:
    -
        class: Symplify\PHPStanRules\Collector\Class_\PublicStaticPropertyCollector
        tags:
            - phpstan.collector

    -
        class: Symplify\PHPStanRules\Collector\StaticPropertyFetch\PublicStaticPropertyFetchCollector
        tags:
            - phpstan.collector

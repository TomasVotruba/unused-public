includes:
    - ../../../../config/included_services.neon

rules:
    - Symplify\PHPStanRules\Rules\DeadCode\UnusedPublicPropertyRule

services:
    -
        class: Symplify\PHPStanRules\Collector\Class_\PublicPropertyCollector
        tags:
            - phpstan.collector

    -
        class: Symplify\PHPStanRules\Collector\PropertyFetch\PublicPropertyFetchCollector
        tags:
            - phpstan.collector
